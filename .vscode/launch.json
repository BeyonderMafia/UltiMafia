{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Chat",
      "port": 9231,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "trace": true,
      "address": "localhost",
      "outputCapture": "console",
      "remoteRoot": "/home/um/",
      "localRoot": "/"
    },
    {
      "name": "Attach to Games",
      "port": 9230,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "trace": true,
      "address": "localhost",
      "outputCapture": "console",
      "remoteRoot": "/home/um/",
      "localRoot": "/"
    },
    {
      "name": "Attach to WWW",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "trace": true,
      "address": "localhost",
      "outputCapture": "console",
      "remoteRoot": "/home/um",
      "localRoot": "/"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "frontend",
      "url": "http://127.0.0.1",
      "webRoot": "${workspaceFolder}/react_main"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "game",
      "skipFiles": ["<node_internals>/**"],
      "enableContentValidation": false,
      "program": "${workspaceFolder}/Games/games.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "www",
      "skipFiles": ["<node_internals>/**"],
      "enableContentValidation": false,
      "program": "${workspaceFolder}/bin/www"
    },
    {
      "args": ["--timeout", "12000", "--recursive", "${workspaceFolder}/test"],
      "internalConsoleOptions": "openOnSessionStart",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
