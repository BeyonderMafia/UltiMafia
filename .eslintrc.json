{
  "env": {
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "script"
  },
  "ignorePatterns": ["node_modules", "react_main"],
  "rules": {
    "prettier/prettier": "error"
  }
}
